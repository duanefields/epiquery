{
  "name": "epiquery",
  "version": "0.0.1",
  "description": "run templated queries from the http's",
  "main": "./bin/epic-server",
  "dependencies": {
    "socket.io": "0.9.13",
    "coffee-script": "1.6.2",
    "express": "3.1.0",
    "docopt": "0.4.0",
    "forever": "0.10.0",
    "underscore": "1.4.4",
    "tedious": "0.1.3",
    "dot": "1.0.1",
    "q": "0.9.2"
  },
  "bin": {

  },
  "config": {
    "db_port":"",
    "db_host":"",
    "db_user":"",
    "db_password":"",
    "template_directory": "",
    "template_repository": "",
    "template_update_interval": "",
    "http_port":"8080"
  },
  "engine": {
    "node": "0.10.1"
  },
  "repository": {
    "type": "git",
    "url":  "git://github.com/intimonkey/epic-query.git"
  },
  "scripts": {
    "start": "node_modules/.bin/forever start --append --watchDirectory ./bin --watch -c ./node_modules/coffee-script/bin/coffee bin/epic-server",
    "debug": "npm run-script watch_templates_for_updates ; node_modules/.bin/forever --append --watchDirectory ./bin --watch -c ./node_modules/coffee-script/bin/coffee bin/epic-server",
    "stop": "node_modules/.bin/forever stop `./node_modules/.bin/forever list | grep epic-server | awk '{ print $2 }' | sed -e 's/\\[//g' | sed -e 's/\\]//g'`",
    "status": "node_modules/.bin/forever list",
    "test": "bin/test",
    "update_templates": "bin/update_or_clone_repo ${npm_package_config_template_directory} ${npm_package_config_template_repository}",
    "predeploy": "",
    "postdeploy": ""
  },
  "os": ["!win32"]
}
